{
  "require": {
    "craftcms/cms": "3.5.14",
    "craftcms/commerce": "3.2.8.1",
    "craftcms/commerce-taxjar": "1.0.0",
    "craftcms/feed-me": "4.2.4",
    "craftcms/redactor": "2.8.3",
    "digital-pros/commerce-authorize": "1.0.12",
    "dolphiq/jsonclient": "1.0.3",
    "hybridinteractive/craft-contact-form-extensions": "1.2.1",
    "markhuot/craftql": "1.3.5",
    "nystudio107/craft-seomatic": "3.3.22",
    "solspace/craft-freeform": "3.8.4",
    "studioespresso/craft-dumper": "2.0.0",
    "verbb/postie": "2.3.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
